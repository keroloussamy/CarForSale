@model IEnumerable<DAL.Employee>

@{
    ViewBag.Title = "Index";
}

<h2>Employees</h2>

<p>
    @Html.ActionLink("Add New Employee", "EmployeeRegister", "Account", new { area = "Accounts" },new { })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Employee Name")
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Address.City)
        </th>
    </tr>
    
@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.User.UserName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.User.Email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.User.Address.City)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @*@Html.ActionLink("Delete", "Delete", new { id = item.Id })*@
    </td>
</tr>
}

</table>
